{
  "name": "@storeforge/unwrap",
  "version": "0.8.0",
  "description": "Tiny utility for \"unwrapping\" Shopify GraphQL responses and handling user errors",
  "license": "MIT",
  "repository": "storeforgedev/unwrap",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "shopify",
    "graphql",
    "unwrap"
  ],
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4",
  "prettier": {},
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "clean": true,
    "format": "esm",
    "sourcemap": true
  }
}
