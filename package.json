{
  "name": "@storeforge/unwrap",
  "version": "0.1.0",
  "description": "Tiny utility for \"unwrapping\" Shopify GraphQL responses and handling user errors",
  "license": "MIT",
  "repository": "storeforgedev/unwrap",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "format": "prettier --write ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "shopify",
    "graphql",
    "unwrap"
  ],
  "peerDependencies": {
    "@shopify/graphql-client": "^1.2.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "packageManager": "pnpm@9.10.0+sha256.355a8ab8dbb6ad41befbef39bc4fd6b5df85e12761d2724bd01f13e878de4b13",
  "prettier": {},
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "clean": true,
    "format": "esm",
    "sourcemap": true
  }
}
